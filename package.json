{
  "name": "@rcb-plugins/llm-connector",
  "pluginLogo": "https://cdn.jsdelivr.net/gh/react-chatbotify-plugins/llm-connector@main/src/assets/logo.webp",
  "homepage": "https://github.com/React-ChatBotify-Plugins/llm-connector",
  "keywords": [
    "react",
    "frontend-library",
    "plugins",
    "gemini",
    "openai",
    "webllm",
    "react-chatbotify",
    "react-chatbotify-plugin"
  ],
  "files": [
    "./dist"
  ],
  "version": "0.3.2",
  "description": "A generic LLM connector for integrating Large Language Models (LLMs) in React ChatBotify!",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite serve",
    "build": "tsc -b && vite build",
    "lint": "eslint --fix \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"{src,test,types}/**/*.{ts,tsx,js,json,md}\"",
    "preview": "vite preview",
    "prepack": "tsc && vite build"
  },
  "author": "tjtanjin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/React-ChatBotify-Plugins/llm-connector"
  },
  "peerDependencies": {
    "@mlc-ai/web-llm": "^0.2.78",
    "react": ">=16.14.0",
    "react-chatbotify": "^2.0.0-beta.39",
    "react-dom": ">=16.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-eslint2": "^5.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.34.7"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "clsx": "^2.1.1",
    "token.js": "^0.7.1"
  }
}
